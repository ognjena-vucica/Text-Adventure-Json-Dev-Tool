id_of_your_soldiers = ds_list_create();
ds_list_add(id_of_your_soldiers,"abakir","mird","hariton","pandza","nagdil");

var id_of_your_soldiers_copy = id_of_your_soldiers;
var soldiers_copy = soldiers;

for (var i=0;i&lt;ds_list_size(id_of_your_soldiers_copy);i++){
    army_soldier = ds_list_find_value(id_of_your_soldiers_copy,i);
    //show_message(army_soldier);
    for (var j=0;j&lt;ds_list_size(soldiers_copy);j++){
        soldier = ds_map_create();
        soldier = ds_list_find_value(soldiers_copy,j);
        //show_message(ds_map_find_value(soldier,"id"));
        if (army_soldier == ds_map_find_value(soldier,"id")){
            //show_message("found one!");
            ds_list_add(army,soldier);
            ds_list_delete(id_of_your_soldiers_copy,i);
            i--;
            ds_list_delete(soldiers_copy,j);
            break;
        }
    }
}

number_of_soldirs_not_found = ds_list_size(id_of_your_soldiers_copy);
if(number_of_soldirs_not_found &gt; 0){
    show_message_custom_debug(string(number_of_soldirs_not_found) + " soldier(s) not found in the list");
}else{
    show_message_custom_debug("All soldiers found!");   
}

soldier_ids_by_ranks = ds_map_create();
