//PRE SVEGA NAPOMENA ZA EDIT JSON prica
//kljuc id je samo komentar, nema nikakvu sistemsku vaznost!
//sistem trazi po redosledu, i to je za njega INDEX
//id je samo komentar za laksu orijentaciju!

choice_numero = 0;
choice_position = 0;
choices_list = ds_list_create();
choices_list = father.choices_list;
choices_list_size = father.choices_list_size;

for(choice_numero = 0; choice_numero &lt; choices_list_size; choice_numero++){
    choice = ds_map_create();
    choice = ds_list_find_value(choices_list, choice_numero);
    jump_to_index = ds_map_find_value(choice,"jump_to_index");
    choice_case_validated = true;
    
    //check choice case to see if case will be shown depending on certain variable conditions
    if (ds_map_exists(choice,"case")){
        choice_case_exists = true;
    }else{
        choice_case_exists = false;
    }
    
    if (choice_case_exists){
        choice_case = ds_map_find_value(choice, "case");
        choice_case_validated = case_validator(choice_case);
    }
    
    //show choice text if case validated or case doesn't exist (==validated still)
    if (choice_case_validated){
        choice_text_bulk = ds_map_find_value(choice, "text");
        choice_text = ds_map_find_value(choice_text_bulk, Game_CTRL.language);
        choice_index = choice_numero;
       
        with (instance_create(father.x, father.text_box.bbox_bottom + (choice_position+1)*80, obj_choice_json)){
            choice_text = other.choice_text;
            choice_index = other.choice_index;
            jump_to_index = other.jump_to_index;
            //show_message_custom_debug("choice_index: " + string(choice_index));
            father = other.father;
        }
        choice_position++;
    }else{
        //alarm[0] = 1;
    }
}




</string>
